welcome.php :::
<?
include('connect.php');
$username=$_POST['username'];
$password=$_POST['password'];
$repeatpassword=$_POST['repeatpassword'];
$email=$_POST['email'];
mysql_query("insert into ajaxlogin(username,password,repeatpassword,email)values('$username','$password','$repeatpassword','$email')")or die(mysql_error());
echo "<script type='text/javascript'>alert('Signed Up Successfully. Please Login');window.location.href='index.php' </script>";
?>
